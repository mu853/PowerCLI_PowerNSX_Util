Attribute VB_Name = "Module1"
Sub 線を引くマクロ()
    Application.ScreenUpdating = False
    
    For Each x In Array(xlEdgeLeft, xlEdgeBottom, xlEdgeRight, xlEdgeTop, xlInsideVertical, xlInsideHorizontal)
        With Selection.Borders(x)
            .LineStyle = xlContinuous
            .ColorIndex = xlAutomatic
            .TintAndShade = 0
            .Weight = xlThin
        End With
    Next x
    
    Dim i, j, r, c As Integer
    For i = 0 To Selection.Rows.Count - 1
        For j = 0 To Selection.Columns.Count - 1
            r = Selection.Row + i
            c = Selection.Column + j
            
            If j = 0 Then
                If i < Selection.Rows.Count - 1 And Cells(r + 1, c).Value = "" Then
                    Cells(r, c).Borders(xlEdgeBottom).LineStyle = xlNone
                End If
            Else
                If Cells(r, c - 1).Borders(xlEdgeBottom).LineStyle = xlNone And Cells(r + 1, c).Value = "" Then
                    Cells(r, c).Borders(xlEdgeBottom).LineStyle = xlNone
                End If
            End If
        Next
    Next
    
    Application.ScreenUpdating = True
End Sub


